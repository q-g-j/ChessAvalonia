			</overlays:OnlineGamePlayerQuit>

			<overlays:ErrorMessage
				ZIndex="20"
				HorizontalAlignment="Center"
				VerticalAlignment="Center"
				IsVisible="{Binding ErrorMessageIsVisible, FallbackValue=false}">
			</overlays:ErrorMessage>
		</Grid>	
	</Grid>
</Grid>
